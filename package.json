{
  "name": "chrome-extension",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "build": "NODE_OPTIONS=--openssl-legacy-provider next build",
    "start": "NODE_OPTIONS=--openssl-legacy-provider next start",
    "relay": "relay-compiler --src ./src --schema ./schema.graphql --extensions js jsx",
    "test": "jest --watch",
    "test:e2e:open": "cypress open",
    "test:e2e:run": "cypress run",
    "schema": "npx postgraphile -c 'postgres://localhost:5432/smooms' --classic-ids --disable-default-mutations --export-schema-graphql=schema.graphql"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.0.13",
    "@chakra-ui/react": "^1.6.2",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@graphile-contrib/pg-many-to-many": "^1.0.1",
    "axios": "^0.21.1",
    "babel-polyfill": "^6.26.0",
    "cookie": "^0.4.1",
    "crypto": "^1.0.1",
    "framer-motion": "^4.1.17",
    "immutability-helper": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "next": "^10.0.9",
    "next-auth": "^3.22.0",
    "pg": "^8.6.0",
    "postgraphile-plugin-nested-mutations": "^1.1.0",
    "qs": "^6.10.1",
    "react": "^17.0.2",
    "react-dnd": "^14.0.3",
    "react-dnd-html5-backend": "^14.0.1",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-relay": "^11.0.2",
    "relay-nextjs": "^0.1.2",
    "zustand": "^3.5.7"
  },
  "devDependencies": {
    "@testing-library/cypress": "^8.0.0",
    "babel-plugin-relay": "^11.0.2",
    "cypress": "^8.4.1",
    "cypress-social-logins": "^1.11.3",
    "graphql": "^15.5.0",
    "jest": "^26.6.3",
    "relay-compiler": "^11.0.2",
    "relay-config": "^11.0.2"
  }
}
