{
  "name": "chrome-extension",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "build": "export SET NODE_OPTIONS=--openssl-legacy-provider && next build",
    "start": "export SET NODE_OPTIONS=--openssl-legacy-provider && next start",
    "relay": "relay-compiler --src ./src --schema ./schema.graphql --extensions js jsx",
    "test": "jest --watch --setupFiles dotenv/config",
    "test:e2e:open": "cypress open",
    "test:e2e:run": "cypress run",
    "schema": "npx postgraphile -c 'postgres://localhost:5432/smooms' --classic-ids --disable-default-mutations --export-schema-graphql=schema.graphql"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.0.13",
    "@chakra-ui/react": "^1.6.2",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@graphile-contrib/pg-many-to-many": "^1.0.1",
    "@loomhq/loom-embed": "^1.2.12",
    "@loomhq/loom-sdk": "^2.28.1",
    "@stripe/stripe-js": "^1.22.0",
    "axios": "^0.21.1",
    "babel-polyfill": "^6.26.0",
    "cookie": "^0.4.1",
    "crypto": "^1.0.1",
    "final-form": "^4.20.4",
    "framer-motion": "^4.1.17",
    "immutability-helper": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "micro": "^9.3.4",
    "micro-cors": "^0.1.1",
    "next": "^10.0.9",
    "next-auth": "^3.22.0",
    "pg": "^8.6.0",
    "pino": "^7.2.0",
    "postgraphile-plugin-nested-mutations": "^1.1.0",
    "qs": "^6.10.1",
    "react": "^17.0.2",
    "react-dnd": "^14.0.3",
    "react-dnd-html5-backend": "^14.0.1",
    "react-dom": "^17.0.2",
    "react-final-form": "^6.5.7",
    "react-icons": "^4.2.0",
    "react-relay": "^11.0.2",
    "relay-nextjs": "^0.1.2",
    "stripe": "^8.191.0",
    "zustand": "^3.5.7"
  },
  "devDependencies": {
    "@testing-library/cypress": "^8.0.2",
    "babel-plugin-relay": "^11.0.2",
    "cypress": "^8.7.0",
    "cypress-social-logins": "^1.13.0",
    "dotenv": "^10.0.0",
    "graphql": "^15.5.0",
    "jest": "^26.6.3",
    "relay-compiler": "^11.0.2",
    "relay-config": "^11.0.2"
  }
}
